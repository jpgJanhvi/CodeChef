#include <iostream>
using namespace std;

int main(){
	long long n,q;
	cin>>n>>q;
	long int a[n],b[n];
	for(int i=0;i<n;i++){
	    cin>>a[i];
	}
	for(int i=0;i<n;i++){
	    cin>>b[i];
	}
	while(q--){
	    long long l,r,i;
	    cin>>l>>r;
	    long long  ans=0;
	    for(i=l-1;i<r;i++){
	        ans=ans+(a[i]*b[i]);
	    }
	    cout<<ans<<endl;
	}
	return 0;
}
